{
  "prettier.enable": false,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "yaml"
  ],
  "cSpell.words": [
    "adsbygoogle",
    "booklist",
    "chapterlist",
    "chokidar",
    "comiclist",
    "comicpage",
    "demi",
    "Deployables",
    "domcontentloaded",
    "effectspeed",
    "esbuild",
    "fanye",
    "historys",
    "isload",
    "KHTML",
    "lazyload",
    "mangas",
    "manhua",
    "mokelao",
    "networkidle",
    "Nuxt",
    "nuxtjs",
    "Orama",
    "presign",
    "Shadcn",
    "starye",
    "turborepo",
    "undici",
    "unocss",
    "upserted",
    "Upserting",
    "VITE",
    "vueuse",
    "workerd",
    "wxqrcode"
  ],
  "conventionalCommits.scopes": [
    "ci",
    "dashboard",
    "crawler"
  ]
}
