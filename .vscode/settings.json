{
  "prettier.enable": false,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "yaml"
  ],
  "cSpell.words": [
    "demi",
    "Deployables",
    "domcontentloaded",
    "esbuild",
    "networkidle",
    "Orama",
    "presign",
    "starye",
    "turborepo",
    "undici",
    "unocss",
    "workerd"
  ]
}
